<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  	
  	<display-name>GREENHOUSE</display-name>
  	
  	<welcome-file-list>
    	<welcome-file>greenHouseHome.jsp</welcome-file>
  	</welcome-file-list>
	
	<servlets-list>
	
		<servlet-logout>
			<servlet>
    			<servlet-name>LogoutServlet</servlet-name>
    			<servlet-class>com.unisa.servlets.LogoutServlet</servlet-class>
			</servlet>
			<servlet-mapping>
    			<servlet-name>LogoutServlet</servlet-name>
    			<url-pattern>/LogoutServlet</url-pattern>
			</servlet-mapping>
		</servlet-logout>
		
		<servlet-registration>
			<servlet>
    			<servlet-name>RegistrationServlet</servlet-name>
    			<servlet-class>com.unisa.servlets.RegistrationServlet</servlet-class>
			</servlet>
			<servlet-mapping>
    			<servlet-name>RegistrationServlet</servlet-name>
    			<url-pattern>/RegistrationServlet</url-pattern>
			</servlet-mapping>
		</servlet-registration>
		
		<servlet-login>
			<servlet>
    			<servlet-name>LoginServlet</servlet-name>
    			<servlet-class>com.unisa.servlets.LoginServlet</servlet-class>
			</servlet>
			<servlet-mapping>
    			<servlet-name>LoginServlet</servlet-name>
    			<url-pattern>/LoginServlet</url-pattern>
			</servlet-mapping>
		</servlet-login>


		<servlet-user-list>
			<servlet>
				<servlet-name>UserListServlet</servlet-name>
				<servlet-class>com.unisa.servlets.UserListServlet</servlet-class>
			</servlet>
			<servlet-mapping>
				<servlet-name>UserListServlet</servlet-name>
				<url-pattern>/UserListServlet</url-pattern>
			</servlet-mapping>
		</servlet-user-list>
	</servlets-list>
	
  	
</web-app>
  


